{
  "name": "maenbn/openamauth-laravel",
  "description": "OpenAM Authentication driver for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Norcombe",
      "email": "b.norcombe@gmail.com"
    }
  ],
  "repositories": [
    {
      "url": "https://github.com/dimoniet/openamauth.git",
      "type": "git"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/contracts": "~5.0",
    "illuminate/support": "~5.0",
    "maenbn/openamauth": "dev-master"
  },
  "require-dev": {
    "orchestra/testbench": "3.2.*",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Maenbn\\OpenAmAuthLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maenbn\\Tests\\OpenAmAuthLaravel\\": "tests/"
    }
  }
}
